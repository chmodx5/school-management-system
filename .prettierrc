{
    "singleQuote": true,
    "plugins": [
        "prettier-plugin-organize-imports",
        "prettier-plugin-tailwindcss",
        "@prettier/plugin-php"
    ],
    "insertFinalNewline": false,
    "semi": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "endOfLine": "auto"
}
